table#tblAlumnos(style="border: 1px solid green")
    thead 
        tr 
            th No control
            th Nombre
            th Carrera 
    tbody 
        each alumnos in [{nc:"12",alumno:"hugo",c:"ISC",},{nc:"13",alumno:"paco",c:"MECA",},{nc:"14",alumno:"luis",c:"ISC",}]
            tr 
                td #{alumnos.nc} 
                td #{alumnos.alumno}
                td #{alumnos.c}
